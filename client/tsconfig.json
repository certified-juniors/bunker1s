{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "module": "CommonJS",
        "moduleResolution": "node",
        "outDir": "./build",
        "rootDir": "./",
        "skipLibCheck": true,
        "strict": true,
        "target": "ESNext",
        "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
